{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "import/prefer-default-export": 0,
        "react/jsx-filename-extension": 0
    },
    "settings": {
        "import/resolver": "webpack"
    }
}